   "source": [
    "p1, p2, p3, p4 = sym.symbols(r\"p_1, p_2, p_3, p_4\")\n",
    "\n",
    "player = [1, p2, p3, p4]\n",
    "\n",
    "r, s, t, p = sym.symbols(\"R, S, T, P\")"
   ]
